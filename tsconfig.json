{
  "compilerOptions": {
    "useUnknownInCatchVariables": false,
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./server/types", "./node_modules/@types"]
  },
  "include": ["./server"]
}
